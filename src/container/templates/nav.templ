{% package templates %}

{% import "container/types" %}

{% templ NavTemplate(apps []types.AppConfig) %}
    <nav data-turbo-permanent="true" id="nav">
        <h1>{%= "Go Feed Me" %}</h1>
        <ul>
            {% for _, a := range apps %}
                <li>
                    <a href={%= a.Url %}  target="_top">{%= a.Name %}</a>
                </li>
            {% endfor %}
        </ul>
    </nav>
{% endtempl %}
